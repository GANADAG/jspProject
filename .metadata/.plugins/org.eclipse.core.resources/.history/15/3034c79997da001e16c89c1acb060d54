<body>
	<h2>Student테이블</h2>
  <button type="button" class="btn btn-success"
  id="btninfo">info 읽기</button>
  
  <div id="show"></div>
  
  <script type="text/javascript">
     $("#btninfo").click(function(){
    	 
    	 $.ajax({
    		 type:"get",
    		 url:"studenttojsonl.jsp",
    		 dataType:"json",
    		 success:function(res){
    			 
    			 var s="";
    			 
    			 $.each(res,function(i,item){
    				 
    				 s+="<div class='alert alert-success' style='width:300px;'>";
    				 s+="index: "+i+"<br>";
    				 s+="<img src='"+item.photo+"' width='100'>";
    				 s+="num: "+item.num+"<br>";
    				 s+="학생명: "+item.name+"<br>";
    				 s+="java: "+item.java+"<br>";
    				 s+="spring: "+item.spring+"<br>";
    				 
    				 s+="</div>";
    			 });
    			 
    			 $("#show").html(s);
    			 
    		 }
    	 })
     });
  </script>
  
  


</body>
</html>